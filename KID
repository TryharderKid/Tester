local Lurnai = loadstring(game:HttpGet("https://raw.githubusercontent.com/TryharderKid/Tester/main/LurnaiTest"))()

local window = Lurnai:CreateWindow("My Cool Script", "v1.0.0")

-- Example elements
local button = Lurnai.Components.CreateButton(window, {
    Text = "Click Me!",
    Callback = function()
        print("Button clicked!")
    end
})

local toggle = Lurnai.Components.CreateToggle(window, {
    Text = "Toggle Feature",
    Default = false,
    Callback = function(value)
        print("Toggle is now:", value)
    end
})
